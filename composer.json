{
    "name": "pedro-santiago/laravel-sentry-api",
    "description": "A description for laravel-sentry-api.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Pedro Santiago",
            "email": "pedro@idez.com.br"
        }
    ],
    "require": {
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "PedroSantiago\\SentryApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PedroSantiago\\SentryApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PedroSantiago\\SentryApi\\SentryApiServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
